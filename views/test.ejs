      $(document).ready(function(){
      $(".fancybox").fancybox({
      openEffect: "none",
      closeEffect: "none"
      });
            
      $(".zoom").hover(function(){
            
      $(this).addClass('transition');
      }, function(){
      $(this).removeClass('transition');
      });
      });
      $(".test-button").click(function(){
         $([document.documentElement, document.body]).animate({
            scrollTop: $(".protect_section").offset().top
         }, 50);
      })
      $(".btn.btn-success:enabled").click(async (e)=>{
         if(e.target.innerHTML != "Submit"){
            curQuestion++;
            let questions = $(".card-body");
            let index = 0;
            for(let question of questions){
               if(index == curQuestion){
                  question.style.display = "initial";
               } else {
                  question.style.display = "none";
               }
               index++;
            }
         } else {
            let valid = true;
            for(let answer of Object.keys(answers)){
               if(answers[answer] == null){
                  console.log(answer);
                  valid = false;
               }
            }
            if(valid){
               let cards = $(".card-body");
               for(let card of cards){
                  card.remove();
               }
               $(".card").append(`
               <div class="card-body" style="display:initial;">
                     <div style="display:flex; flex-direction: column; justify-content:center; align-items: center;">
                        <image src="/images/tick.png" style="width: 10em; height:10em; margin: 3em 0em;"></image>
                        <h2 style="margin-bottom: 2em;">Test has been submitted!</h2>
                        <div id="finishedInfo">

                        </div>
                     </div>
               </div>
               `)
               let req = await fetch("http://localhost:6969/endofdaytest", {
                  method: "POST",
                  headers:{
                     "Content-Type":"application/json",
                  },
                  body: JSON.stringify(answers)
               })
               let fetchResponse = await req.json();
               $("#finishedInfo").append(JSON.stringify(`<p>Potential Problem: ${fetchResponse.disease} <a style=color:#1a0dab;margin-left:1em; href=https://www.google.com/search?q=${fetchResponse.disease.replace(" ", "+")}>Find out more...</a></p><p>Quote of the day: ${fetchResponse.quote}</p>`));
            } else {
               alert("Unfilled Question");
            }
         }
      })
      $(".btn.btn-danger:enabled").click(()=>{
         curQuestion--;
         let questions = $(".card-body");
         let index = 0;
         for(let question of questions){
            if(index == curQuestion){
               question.style.display = "initial";
            } else {
               question.style.display = "none";
            }
            index++;
         }
      })
      $("area").click(function(e){
         if(answers.question2.indexOf(e.target.title) == -1){
            answers.question2.push(e.target.title);
         } else {
            answers.question2.splice(answers.question2.indexOf(e.target.title), 1);
         }
         if(answers.question2.length == 0){
            $("#bodyForm").html("");
         }
         for(let i = 0; i < answers.question2.length; i++){
            if(i == 0){
               $("#bodyForm").html(`
            <input class="form-control" aria-describedby="emailHelp" placeholder="Enter your ${answers.question2[i]} problem..." min="0" style="width: 100%; display:inline-block; margin-bottom: 2em;">
            `);
            } else {
            $("#bodyForm").append(`
            <input class="form-control" aria-describedby="emailHelp" placeholder="Enter your ${answers.question2[i]} problem..." min="0" style="width: 100%; display:inline-block; margin-bottom: 2em;">
            `);
            }
            $("#bodyForm input").keyup(function(e){
               let topVal = $(e.target).offset().top + 38;
               let words = 0;
               let autos = $(".autocomplete");
               for(let auto of autos){
                  auto.remove();
               }
               for(let j = 0; j < symptomArr.length; j++){
                  if(symptomArr[j].Name.toLowerCase().includes(e.target.value.toLowerCase())){
                     words++;
                     $(document.body).append(`<div class="autocomplete" style="position:absolute; top: ${topVal + ((words-1) * 40)}px; width: 100%; display:flex; justify-content:center;"><div style="cursor: pointer; width: 50%; padding-top: 7px; height: 40px; background-color:white; border: 1px solid black;" data-id="${i}">` + symptomArr[j].Name + "</div></div>")
                     console.log(symptomArr[j].Name);
                  }
                  if(words > 10){
                     break;
                  }
               }
               $(".autocomplete div").hover(function(){
                    $("#bodyForm input").focusout(function(e){})
                  $(this).css("background-color", "#d9d9d9");
               }, function(){
                  $(this).css("background-color", "white");
                  $(document.body).click(function(e){
                    if($(e.target).data("id") == undefined){
                        let autos = $(".autocomplete");
                        for(let auto of autos){
                            auto.remove();
                        }
                    }
                    })
               });
               $(".autocomplete div").click(function(){
                   let inputs = $("#bodyForm input");
                   for(let ob in inputs){
                    if(ob == $(this).data("id")){
                        $(inputs[ob]).val($(this).text());
                    }
                   }
                   
               })
            })
         }
      })
      $("#digitalDeviceTime").change(function(e){
         answers.question1 = e.target.value;
      })
      $("#sleepingTime").change(function(e){
         answers.question4 = e.target.value;
      })
